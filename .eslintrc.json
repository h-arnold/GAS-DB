{
  "env": {
    "googleappsscript/googleappsscript": true,
    "es6": true
  },
  "plugins": [
    "googleappsscript"
  ],
  "extends": [
    "eslint:recommended",
    "google"
  ],
  "rules": {
    // Add project-specific rules here
    "max-len": ["warn", { "code": 120 }],
    "require-jsdoc": "off",
    "valid-jsdoc": "off",
    "no-unused-vars": ["warn", { "args": "none" }],
    "no-console": "off"
  }
}
